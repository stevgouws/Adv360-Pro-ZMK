macro_kinesis: macro_kinesis{
compatible = "zmk,behavior-macro";
label = "macro_kinesis";
#binding-cells = <0>;
bindings = <&kp LS(K)>, <&kp I>, <&kp N>, <&kp E>, <&kp S>, <&kp I>, <&kp S>;
};
macro_hyper: macro_hyper{
compatible = "zmk,behavior-macro";
label = "macro_hyper";
#binding-cells = <0>;
bindings = <&macro_press &mo 4 &kp LSHFT &kp LALT &kp LCTRL &kp LCMD>, <&macro_release &mo 4 &kp LSHFT &kp LALT &kp LCTRL &kp LCMD>;
};
macro_hyper: macro_hyper{
compatible = "zmk,behavior-macro";
label = "macro_hyper";
#binding-cells = <0>;
bindings = <&macro_press &mo 5 &kp LSHFT &kp LALT &kp LCTRL &kp LCMD>, <&macro_pause_for_release>, <&macro_release &mo 5 &kp LSHFT &kp LALT &kp LCTRL &kp LCMD>;
};