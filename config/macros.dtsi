macro_arrow: macro_arrow{
compatible = "zmk,behavior-macro";
label = "macro_arrow";
#binding-cells = <0>;
bindings = <&kp SPACE>, <&kp EQUAL>, <&kp GREATER_THAN>, <&kp SPACE>;
};
macro_cmd_tilde: macro_cmd_tilde{
compatible = "zmk,behavior-macro";
label = "macro_cmd_tilde";
#binding-cells = <0>;
bindings = <&macro_press &kp LCMD>, <&macro_tap &kp GRAVE>, <&macro_release &kp LCMD>;
};
macro_quotes: macro_quotes{
compatible = "zmk,behavior-macro";
label = "macro_quotes";
#binding-cells = <0>;
bindings = <&kp SQT>, <&kp SQT>, <&kp LEFT>;
};
